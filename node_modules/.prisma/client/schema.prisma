generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x", "windows"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Food {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  breakfast String
  lunch     String
  dinner    String
}
